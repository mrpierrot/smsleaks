{
  "name": "smsleaks",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": ""
  },
  "project": {
    "source": "./src",
    "build": "./build",
    "dist": "./dist",
    "bundle": {
      "main": "/app/app.js",
      "dest": "/app/bundle.js",
      "assets": "/assets",
      "css": "/app/css"
    },
    "server": {
      "port": 1983,
      "livereload-port": 35729
    }
  },
  "license": "GPL-3.0",
  "browser": {
    "angular": "./node_modules/angular/angular.min.js",
    "angular-route": "./node_modules/angular-ui-router/release/angular-ui-router.min.js",
    "angular-bootstrap": "./node_modules/angular-ui-bootstrap/ui-bootstrap.min.js",
    "angular-animate": "./node_modules/angular-animate/angular-animate.min.js",
    "jquery": "./node_modules/jquery/dist/jquery.min.js",
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.min.js",
    "gsap": "./node_modules/gsap/src/minified/TweenLite.min.js"
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "gsap": {
      "exports": "gsap"
    },
    "bootstrap": {
      "exports": "bootstrap",
      "depends": [
        "jquery"
      ]
    },
    "angular": {
      "exports": "angular",
      "depends": [
        "jquery"
      ]
    },
    "angular-route": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "angular-animate": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "angular-bootstrap": {
      "exports": null,
      "depends": [
        "angular",
        "bootstrap"
      ]
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "dependencies": {
    "angular": "^1.4.6",
    "angular-ui-bootstrap": "^0.13.4",
    "angular-ui-router": "^0.2.15",
    "bootstrap": "^3.3.5",
    "jquery": "^2.1.4"
  },
  "devDependencies": {
    "angular-animate": "^1.4.6",
    "browserify": "^11.1.0",
    "browserify-shim": "^3.8.10",
    "del": "^2.0.2",
    "gsap": "^1.18.0",
    "gulp": "^3.9.0",
    "gulp-connect": "^2.2.0",
    "gulp-gzip": "^1.2.0",
    "gulp-if": "^1.2.5",
    "gulp-less": "^3.0.3",
    "gulp-livereload": "^3.8.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-template": "^3.0.0",
    "gulp-uglify": "^1.4.1",
    "gulp-util": "^3.0.6",
    "less": "^2.5.2",
    "malihu-custom-scrollbar-plugin": "^3.0.9",
    "minimist": "^1.2.0",
    "run-sequence": "^1.1.3",
    "stringify": "^3.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.4.0"
  }
}
